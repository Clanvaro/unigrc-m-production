mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "NODE_ENV=development npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[agent]
integrations = [
  "javascript_database:1.0.0",
  "javascript_object_storage:1.0.0",
  "javascript_log_in_with_replit:1.0.0",
  "javascript_sendgrid:1.0.0",
